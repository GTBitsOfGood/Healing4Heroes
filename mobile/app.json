{
  "expo": {
    "name": "Healing4Heroes",
    "slug": "healing4heroes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "newArchEnabled": true,
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow Healing4Heroes to access your photos.",
        "NSPhotoLibraryAddUsageDescription": "Allow Healing4Heroes to save photos."
      },
      "bundleIdentifier": "org.healing4heroes"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Healing4Heroes to access your photos."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Healing4Heroes to access your photos.",
          "savePhotosPermission": "Allow Healing4Heroes to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "2fe0574b-fe03-4d69-bb5d-a3fb731c1aa3"
      }
    },
    "owner": "bitsofgood",
    "android": {
      "package": "org.healing4heroes"
    }
  }
}
